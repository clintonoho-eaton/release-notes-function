{
    "schema": 1,
    "description": "Generate release notes for a bug",
    "execution_settings": {
      "default": {
        "max_completion_tokens": 1000,
        "temperature": 1.0,
        "top_p": 1.0,
        "presence_penalty": 0.0,
        "frequency_penalty": 0.0
      }
    },
    "input_variables": [
      {
        "name": "issue_type",
        "description": "Jira Ticket Type",
        "default": "issue"
      },
      {
        "name": "issue_info",
        "description": "Jira Ticket Details",
        "default": ""
      },
      {
        "name": "output_schema",
        "description": "indicator to describe format output",
        "default": "{output_schema}"
      }
    ]
  }
  